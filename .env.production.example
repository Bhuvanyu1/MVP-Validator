# Production Environment Variables for Vercel Frontend

# API Configuration
VITE_API_BASE_URL=https://mvp-validator-worker.your-subdomain.workers.dev
VITE_ENVIRONMENT=production

# Stripe (Frontend)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key

# Sentry (Frontend Error Monitoring)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_MONITORING=true
VITE_ENABLE_ERROR_REPORTING=true

# App Configuration
VITE_APP_NAME="MVP Validator"
VITE_APP_VERSION="1.0.0"
VITE_SUPPORT_EMAIL="support@yourdomain.com"

# OAuth Configuration (if using frontend OAuth)
VITE_GOOGLE_CLIENT_ID=your-google-oauth-client-id

# Performance Monitoring
VITE_PERFORMANCE_MONITORING_ENABLED=true
VITE_LOG_LEVEL=error
